{
    "name": "danielgp/salariu",
    "description": "Salariu conform legilor din Romania (Salary according to Romanian laws)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Popiniuc",
            "email": "danielpopiniuc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "components/flag-icon-css": ">=2.6.0",
        "danielgp/bank-holidays": ">=1.4.5",
        "danielgp/common-lib": ">=3.0.2",
        "danielgp/common-styles": ">=1.1.5",
        "fortawesome/font-awesome": ">=4.6.3",
        "gettext/gettext": ">=4.1.0"
    },
    "autoload": {
        "psr-4": {
            "danielgp\\bank_holidays\\": "vendor/danielgp/bank-holidays/",
            "danielgp\\common_lib\\": "vendor/danielgp/common-lib/"
        },
        "classmap": [
            "Salariu/Bonuses.php",
            "Salariu/ForeignCurrency.php",
            "Salariu/Taxation.php",
            "Salariu/BasicSalariu.php",
            "Salariu/Salariu.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": ["/nbproject", "*.po", "/scss", "/js/tabber.js", "/vendor", "/tests"]
    }
}
