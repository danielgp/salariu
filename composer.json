{
    "name": "danielgp/salariu",
    "description": "Salariu conform legilor din Romania (Salary according to Romanian laws)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Popiniuc",
            "email": "danielpopiniuc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1 || ^7.2",
        "components/flag-icon-css": "^3",
        "danielgp/bank-holidays": "^1",
        "danielgp/common-lib": "^3",
        "danielgp/common-styles": "^1",
        "fortawesome/font-awesome": "^4",
        "gettext/gettext": "^4"
    },
    "autoload": {
        "psr-4": {
            "danielgp\\bank_holidays\\": "vendor/danielgp/bank-holidays/",
            "danielgp\\common_lib\\": "vendor/danielgp/common-lib/"
        },
        "classmap": [
            "Salariu/Bonuses.php",
            "Salariu/ForeignCurrency.php",
            "Salariu/Taxation.php",
            "Salariu/TaxationHealth.php",
            "Salariu/InputValidation.php",
            "Salariu/BasicSalariu.php",
            "Salariu/FormattingSalariu.php",
            "Salariu/Salariu.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": [
            ".gitignore",
            "interfaceElements.json",
            "valuesToCompute.json",
            "*.bat",
            "*.po",
            "*.vbs",
            "/.projectKnowledge",
            "/nbproject",
            "/Salariu/scss",
            "/sourceforge.net",
            "/results",
            "/vendor",
            "/tests"
        ]
    }
}


