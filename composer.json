{
    "name": "danielgp/salariu",
    "description": "Salariu conform legilor din Romania (Salary according to Romanian laws)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Popiniuc",
            "email": "danielpopiniuc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "danielgp/common-lib": ">=1.1.7",
        "inetsys/phpgettext": ">=1.0.11"
    },
    "autoload": {
        "psr-4": {
            "": "Salariu/",
            "danielgp\\common_lib\\": "vendor/danielgp/common-lib/"
        },
        "classmap": [
            "Salariu/Bonuses.php",
            "Salariu/Taxation.php",
            "Salariu/Salariu.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": ["/nbproject", "*.po", "/scss", "/js/tabber.js", "/vendor", "/tests"]
    }
}
